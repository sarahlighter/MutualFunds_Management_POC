package com.demo.financeproducts.model;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.EntityListeners;
import javax.persistence.Table;

import org.springframework.data.jpa.domain.support.AuditingEntityListener;

@Entity
@Table(name = "users")
@EntityListeners(AuditingEntityListener.class)
public class MutualFund {
	private long id;
	private String name;
	private String accountType;
	private String assetClass;
	private Date inceptionsData;
	private String ticker;
	private String shareClass;
	private String CUSIP;
	private Date portfolioInceptionDate;
	private String dividendFrequency;
	private double turnoverRatio;
	private String turnoverRatioDescription;
	private String benchmarkAndIndices;
	private int numberOfHoldings;
	private double returnOnEquity;
	
}
